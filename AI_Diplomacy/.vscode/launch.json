{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Chrome Debug 5179",
      "url": "http://localhost:5179",
      "webRoot": "${workspaceFolder}/ai_animation/",
      "sourceMapPathOverrides": {
        "http://localhost:5179/*": "${webRoot}/*"
      },
      "runtimeArgs": [
        "--remote-debugging-port=9223"
      ],
      "sourceMaps": true
    }
  ]
}
